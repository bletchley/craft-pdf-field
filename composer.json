{
    "name": "bletchley/pdf-field",
    "description": "Requests a new PDF for a product on save",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "pdf field"
    ],
    "support": {
        "docs": "https://github.com/https://github.com/bletchley/craft-pdf-field/pdf-field/blob/master/README.md",
        "issues": "https://github.com/https://github.com/bletchley/craft-pdf-field/pdf-field/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Andy Skogrand",
            "homepage": "http://bletchley.co/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "bletchley\\pdffield\\": "src/"
        }
    },
    "extra": {
        "name": "PDF Field",
        "handle": "pdf-field",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/https://github.com/bletchley/craft-pdf-field/pdf-field/master/CHANGELOG.md",
        "components": {
            "pdfserviceresponse": "bletchley\\pdffield\\services\\Pdfserviceresponse"
        },
        "class": "bletchley\\pdffield\\PdfField"
    }
}
